@model List<LessonViewModel>
@{
    ViewData["Title"] = "Tutoring Ana Sayfa";
}
<div class="row">
    <div class="col-2">
        @await Component.InvokeAsync("Categories")
     
    </div>
    <div class="col-10">
        <div class="row g-3">
            @foreach (var lessonViewModel in Model)
            {
                <div class="col-2">
                    @await Html.PartialAsync("_LessonPartial",lessonViewModel)
                </div>
            }
        </div>
    </div>
</div>
